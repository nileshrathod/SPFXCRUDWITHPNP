{
  "$schema": "https://dev.office.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "spfxcrudwp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spfxcrudwp/SpfxcrudwpWebPart.js",
          "manifest": "./src/webparts/spfxcrudwp/SpfxcrudwpWebPart.manifest.json"
        }
      ]
    },
    "view-employee-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/viewEmployee/ViewEmployeeWebPart.js",
          "manifest": "./src/webparts/viewEmployee/ViewEmployeeWebPart.manifest.json"
        }
      ]
    },
    "edit-employee-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/editEmployee/EditEmployeeWebPart.js",
          "manifest": "./src/webparts/editEmployee/EditEmployeeWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "node_modules/jquery/dist/jquery.min.js",
      "globalName": "jQuery"
    },
    "jqueryui": {
      "path": "node_modules/jqueryui/jquery-ui.min.js",
      "globalName": "jQuery"
    },
    "bootstrap": {
      "path": "node_modules/bootstrap/dist/js/bootstrap.min.js",
      "globalName": "jQuery"
    },
    "moment": {
      "path": "node_modules/moment/moment.js",
      "globalName": "moment"
    },
    "toastr": {
      "path": "node_modules/toastr/toastr.js",
      "globalName": "jQuery"
    }
  },
  "localizedResources": {
    "SpfxcrudwpWebPartStrings": "lib/webparts/spfxcrudwp/loc/{locale}.js",
    "ViewEmployeeWebPartStrings": "lib/webparts/viewEmployee/loc/{locale}.js",
    "EditEmployeeWebPartStrings": "lib/webparts/editEmployee/loc/{locale}.js"
  }
}
